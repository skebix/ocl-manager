	 rdfs:comment="Agrupamiento bajo una denominación y grado común"
	 rdfs:label="kb:clase">
	<rdfs:domain rdf:resource="&kb;Cargo"/>
	<rdfs:range rdf:resource="&kb;Clase"/>
</rdf:Property>
<rdf:Property rdf:about="&kb;codigo"
