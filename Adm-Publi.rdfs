	 rdfs:label="kb:semantic_type">
	<rdfs:domain rdf:resource="&kb;KB"/>
	<rdfs:domain rdf:resource="&kb;KB_ROOT"/>
	<rdfs:range rdf:resource="&rdfs;Literal"/>
</rdf:Property>
